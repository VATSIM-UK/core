{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/VATSIM-UK/slack"
        },
        {
            "type": "vcs",
            "url": "https://github.com/VATSIM-UK/VatsimXML"
        },
        {
            "type": "vcs",
            "url": "https://github.com/VATSIM-UK/VatsimSSO"
        },
        {
            "type": "vcs",
            "url": "https://github.com/VATSIM-UK/laravel-ipboard"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "vatsim/xml": "dev-master",
        "vatsim/sso": "dev-develop",
        "guzzlehttp/guzzle": "~6.0",
        "doctrine/dbal": "~2.5",
        "barryvdh/laravel-debugbar" : "~3.0",
        "planetteamspeak/ts3-php-framework": "dev-master",
        "laravelcollective/html": "~5.0",
        "skymeyer/vatsimphp" : "1.0.*",
        "maknz/slack": "@dev",
        "league/csv": "^8.0",
        "alawrence/laravel-ipboard": "dev-master",
        "vluzrmos/slack-api": "dev-master",
        "patricktalmadge/bootstrapper" : "~5",
        "malahierba-lab/public-id": "dev-master",
        "pusher/pusher-php-server": "~3.0",
        "bugsnag/bugsnag-laravel": "^2.0",
        "watson/rememberable": "^2.0",
        "barryvdh/laravel-ide-helper": "^2.1",
        "laravel/browser-kit-testing": "^1.0",
        "laravel/passport": "^4.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0",
        "laravel/homestead": "^3.0",
        "laracasts/generators": "^1.1",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravelcollective/html"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "ide-helper": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:models -RW",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
