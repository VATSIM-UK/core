{
    "name": "vatsimuk/core",
    "description": "VATSIM UK Core Framework",
    "keywords": [
        "vatsim",
        "vatsimuk",
        "vatsim-uk",
        "vatsim uk",
        "planes",
        "aviation"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Anthony Lawrence",
            "email": "artisan@anthonylawrence.me.uk",
            "homepage": "https://github.com/A-Lawrence"
        },
        {
            "name": "Neil Farrington",
            "homepage": "https://github.com/N-Farrington"
        },
        {
            "name": "Joe Clifford"
        },
        {
            "name": "George Jones"
        },
        {
            "name": "Calum Towers"
        },
        {
            "name": "Andy Ford"
        },
        {
            "name": "Alex Toff"
        },
        {
            "name": "Craig Milne"
        },
        {
            "name": "Callum Axon"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/VATSIM-UK/slack"
        },
        {
            "type": "vcs",
            "url": "https://github.com/VATSIM-UK/VatsimXML"
        },
        {
            "type": "vcs",
            "url": "https://github.com/VATSIM-UK/VatsimSSO"
        },
        {
            "type": "vcs",
            "url": "https://github.com/VATSIM-UK/laravel-ipboard"
        },
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/WaitingListsManager"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "alawrence/laravel-ipboard": "dev-master",
        "barryvdh/laravel-debugbar": "~3.0",
        "barryvdh/laravel-ide-helper": "^2.1",
        "benjaminhirsch/nova-slug-field": "^1.1",
        "bugsnag/bugsnag-laravel": "^2.0",
        "cobaltgrid/vatsim-stand-status": "^1.0",
        "doctrine/dbal": "~2.5",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/browser-kit-testing": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/nova": "*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.6",
        "league/csv": "^8.0",
        "maatwebsite/excel": "~2.1.0",
        "malahierba-lab/public-id": "dev-master",
        "planetteamspeak/ts3-php-framework": "1.1.28",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "~3.0",
        "skymeyer/vatsimphp": "1.0.*",
        "spatie/laravel-cookie-consent": "^2.4",
        "spatie/laravel-permission": "^2.23",
        "vatsim/sso": "dev-develop#7579e9f492d84f04bfd30df41f0073b3d7a01527",
        "vatsim/xml": "dev-master",
        "vluzrmos/slack-api": "dev-master",
        "vyuldashev/nova-permission": "^1.4",
        "watson/rememberable": "2.0.3",
        "vatsimuk/waiting-lists-manager": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "symfony/dom-crawler": "^4.0",
        "symfony/css-selector": "^4.0",
        "laravel/homestead": "^7.8.0",
        "laracasts/generators": "^1.1",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravelcollective/html"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "ide-helper": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:models -RW",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
  "minimum-stability": "dev",
  "prefer-stable": true
}
